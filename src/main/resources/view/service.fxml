      </Label>
      <Label layoutX="496.0" layoutY="199.0" style="-fx-background-color: black; -fx-background-radius: 1;" text="PET ID" textFill="WHITE">
         <font>
            <Font name="DejaVu Sans Condensed Bold" size="22.0" />
         </font>
      </Label>
      <TextField fx:id="serviceID" layoutX="260.0" layoutY="195.0" prefHeight="35.0" prefWidth="205.0" />
      <TextField fx:id="cost" layoutX="256.0" layoutY="380.0" prefHeight="35.0" prefWidth="205.0" />
      <TextField fx:id="duration" layoutX="257.0" layoutY="316.0" prefHeight="35.0" prefWidth="205.0" />
      <TextField fx:id="serviceType" layoutX="257.0" layoutY="257.0" prefHeight="35.0" prefWidth="205.0" />
      <ComboBox fx:id="PETid" editable="true" layoutX="620.0" layoutY="200.0" prefWidth="150.0" promptText="select a Pet ID" />
      <Button fx:id="save" layoutX="73.0" layoutY="447.0" mnemonicParsing="false" onAction="#saveAction" prefHeight="35.0" prefWidth="136.0" style="-fx-background-color: #d35400; -fx-background-radius: 23;" text="SAVE" textFill="WHITE">
