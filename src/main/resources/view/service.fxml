      </ImageView>
      <Label layoutX="270.0" layoutY="64.0" style="-fx-background-color: black;" text="SERVICE DETIALS" textFill="WHITE">
         <font>
            <Font name="DejaVu Sans Bold Oblique" size="35.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" layoutX="84.0" layoutY="256.0" style="-fx-background-color: black; -fx-background-radius: 1;" text="SERVICE TYPE" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="DejaVu Sans Condensed Bold" size="22.0" />
         </font>
      </Label>
      <Label alignment="TOP_LEFT" layoutX="89.0" layoutY="199.0" style="-fx-background-color: black; -fx-background-radius: 1;" text="SERVICE ID" textAlignment="CENTER" textFill="WHITE">
